<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.iata.org/IATA/2007/00" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.iata.org/IATA/2007/00" elementFormDefault="qualified" version="3.001" id="IATA2017.1">
	<xs:include schemaLocation="IATA_SimpleTypes.xsd"/>
	<xs:include schemaLocation="IATA_CommonTypes.xsd"/>
	<xs:include schemaLocation="IATA_ET_CommonTypes.xsd"/>
	<xs:element name="IATA_TicketNeutralIssueRQ">
		<xs:annotation>
			<xs:documentation>This message allows the original or initial issuance of an electronic ticket/document.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="POS" type="POS_Type">
					<xs:annotation>
						<xs:documentation>Information to identifty the issuer of the ticket/document.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AgentID" type="UniqueID_Type" minOccurs="0" maxOccurs="2">
					<xs:annotation>
						<xs:documentation>The booking and/or servicing agent id.  Refer to IATA PADIS  Code List 9893 for the code value for Type. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BookingTicketingRefID" type="UniqueID_Type" minOccurs="0" maxOccurs="2">
					<xs:annotation>
						<xs:documentation>The PNR reference.  Type = 1 in IATA PADIS codelist for data element 1153, ID = record locator, and CompanyName/Code = airline or CRS code. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="IssuingAgencyAirlineInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Name and place of issue of the agency or airline issuing the ticket/document.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Name" type="StringLength1to71" use="optional">
							<xs:annotation>
								<xs:documentation xml:lang="en">The name of the issuing agency or airline.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="Place" type="StringLength1to71" use="optional">
							<xs:annotation>
								<xs:documentation xml:lang="en">The place where the document was issued.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Passenger" maxOccurs="99">
					<xs:annotation>
						<xs:documentation>The ticket information for each passenger.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Name">
								<xs:annotation>
									<xs:documentation>The passenger name.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="PersonNameType">
											<xs:attributeGroup ref="TravelerCountGroup">
												<xs:annotation>
													<xs:documentation>use code for ptc.  </xs:documentation>
												</xs:annotation>
											</xs:attributeGroup>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="AgentID" type="UniqueID_Type" minOccurs="0" maxOccurs="3">
								<xs:annotation>
									<xs:documentation>The booking and/or servicing agent id.  Refer to IATA PADIS  Code List 9893 for the code value for Type. </xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PaymentDetail" minOccurs="0" maxOccurs="99">
								<xs:annotation>
									<xs:documentation>Form of payment information.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="PaymentDetailType">
											<xs:sequence>
												<xs:element name="Sponsor" type="FreeTextType" minOccurs="0" maxOccurs="5">
													<xs:annotation>
														<xs:documentation>Name, address, and telephone number of purchaser when other than the passenger.</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="PaymentType" type="IATA_CodeType" use="required">
												<xs:annotation>
													<xs:documentation xml:lang="en">The type of form of payment, e.g. credit card, cash.  Refer to IATA PADIS code list data element 9888.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="CertificateNbr" type="AlphaNumericStringLength1to14" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">A number assigned to a certificate which was surrendered at the time of the original ticket sale.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="Qualifier" type="IATA_CodeType" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">Used to specify if this is the old, new, or orginal form of payment.  Refer to IATA  PADIS code list 9988.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="Text" type="StringLength1to70" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">Free text for the form of payment.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="OriginDestination" minOccurs="0">
								<xs:annotation>
									<xs:documentation>True origin and destination.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attributeGroup ref="OriginDestinationGroup">
										<xs:annotation>
											<xs:documentation xml:lang="en">The true origin and destination for the ticket.</xs:documentation>
										</xs:annotation>
									</xs:attributeGroup>
								</xs:complexType>
							</xs:element>
							<xs:element name="CustLoyalty" minOccurs="0" maxOccurs="5">
								<xs:annotation>
									<xs:documentation>Frequent flyer reference.  ProgramID=airline code, MembershipID=FF number.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attributeGroup ref="CustomerLoyaltyGroup"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="Endorsement" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Endorsement/restriction remarks.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="Info" type="StringLength1to255" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">Remarks provided to ensure common understanding between the passenger and an airline when a fare is resricted.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="AddlReferenceID" type="UniqueID_Type" minOccurs="0" maxOccurs="5">
								<xs:annotation>
									<xs:documentation>ID contains the actual identifier  - invoice, airline confirmation number, customer id, foid or airline indexing field.  Refer to IATA PADIS Code List for data element 9958 for the code value for Type.  If FOID information is being sent, the type of FOID as defined by AIRIMP is sent in ID_Context.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="BaseFare" maxOccurs="3">
								<xs:annotation>
									<xs:documentation>Used to specify the base fare, the base fare net and the base fare sell amounts.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attributeGroup ref="CurrencyAmountGroup">
										<xs:annotation>
											<xs:documentation xml:lang="en">Used to specify the base, base net, or base sell fare.</xs:documentation>
										</xs:annotation>
									</xs:attributeGroup>
									<xs:attribute name="Purpose" type="PurposeType" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">Used to specify if just base or base fare net or base fare sell.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="FareAmountType" type="FareAmountType" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">Used to specify if ticket fare is bulk ticket, inclusive tour, or no additional charge.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="EquivFare" minOccurs="0" maxOccurs="3">
								<xs:annotation>
									<xs:documentation>Used to specify the equivalent , equivalent net or equivalent sell paid fare.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="Purpose" type="PurposeType" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">Used to specify if just equivalent fare paid or paid net or paid sell.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attributeGroup ref="CurrencyAmountGroup">
										<xs:annotation>
											<xs:documentation xml:lang="en">Used to specify the base, base net, or base sell fare.</xs:documentation>
										</xs:annotation>
									</xs:attributeGroup>
									<xs:attribute name="FareAmountType" type="FareAmountType" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">Used to specify if ticket fare is bulk ticket, inclusive tour, or no additional charge.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="TotalFare" maxOccurs="3">
								<xs:annotation>
									<xs:documentation>Used to specify the total, total net, or total sell fare.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="Purpose" type="PurposeType" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">Used to specify if total amount, total net or total sell.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attributeGroup ref="CurrencyAmountGroup">
										<xs:annotation>
											<xs:documentation xml:lang="en">The total, total net, or total sell fare.</xs:documentation>
										</xs:annotation>
									</xs:attributeGroup>
									<xs:attribute name="FareAmountType" type="FareAmountType" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">Used to specify if ticket fare is bulk ticket, inclusive tour, or no additional charge.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ExchangeRate" type="xs:decimal">
										<xs:annotation>
											<xs:documentation>The banker's selling rate used to convert the base amount into the equivalent amount.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="Taxes" minOccurs="0">
								<xs:annotation>
									<xs:documentation>A collection of taxes.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Tax" maxOccurs="99">
											<xs:annotation>
												<xs:documentation>The tax applicable to the ticket/document.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attributeGroup ref="FeeTaxGroup">
													<xs:annotation>
														<xs:documentation xml:lang="en">Used to specify type of tax, amount, currency code and whether new or paid tax.</xs:documentation>
													</xs:annotation>
												</xs:attributeGroup>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="UnstructuredFareCalcInfo" minOccurs="0" maxOccurs="3">
								<xs:annotation>
									<xs:documentation>Fare calculation area, net amount, sell amount, pricing indicator, and reporting indicator.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attributeGroup ref="UnstructuredFareCalcGroup"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="FareInfo" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Account code, net reporting code, nonendorsable and nonrefundable indicators, penalty restriction indicator, pricing system, statistical code, tour code and the around the world fare indicator.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="ET_FareInfo">
											<xs:sequence>
												<xs:element name="PenaltyAmount" type="VoluntaryChangesType" minOccurs="0" maxOccurs="3">
													<xs:annotation>
														<xs:documentation>Non-refundable, no value, penalty amounts.</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="AccountCode" type="StringLength1to32" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">A qualifier for pricing to select fares.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="NonEndorsableInd" type="xs:boolean" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">When true, the fare is nonendorsable.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="NonRefundableInd" type="xs:boolean" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">When true, the fare is non refundable.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attribute name="PentaltyRestrictionInd" type="xs:boolean" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">When true, a penalty restriction applies.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
											<xs:attributeGroup ref="CompanyID_AttributesGroup">
												<xs:annotation>
													<xs:documentation xml:lang="en">A two/three character, IATA assigned, code identifying the system used to price the itinerary for the ticket being issued.</xs:documentation>
												</xs:annotation>
											</xs:attributeGroup>
											<xs:attribute name="AroundTheWorldFareInd" type="xs:boolean" use="optional">
												<xs:annotation>
													<xs:documentation xml:lang="en">When true, this fare is classifed as a published/promotional 'Around the World' fare.</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="TicketDocument" maxOccurs="4">
								<xs:annotation>
									<xs:documentation>Ticket/document number, type, adult and child tkt connection numbers, in connection number, and date of issue.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="CouponInfo" maxOccurs="4">
											<xs:annotation>
												<xs:documentation>Number, status, in connection nbr, itinerary sequence nbr, coupon ref, fare basis code, not valid before/after, free baggage allowance.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="CouponInfoType">
														<xs:sequence>
															<xs:element name="AddlBaggageInfo" type="AddlBaggageInfoType" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>Carry-on and checked baggage allowances.</xs:documentation>
																</xs:annotation>
															</xs:element>
															<xs:element name="InConnectionWithInfo" minOccurs="0" maxOccurs="99">
																<xs:annotation>
																	<xs:documentation>Document and coupon informaton related to this coupon.</xs:documentation>
																</xs:annotation>
																<xs:complexType>
																	<xs:attribute name="InConnectionDocNbr" type="StringLength1to16">
																		<xs:annotation>
																			<xs:documentation>A document number representing a transaction that is issued in relation to this coupon.</xs:documentation>
																		</xs:annotation>
																	</xs:attribute>
																	<xs:attribute name="InConnectionNbr" type="Numeric1to4">
																		<xs:annotation>
																			<xs:documentation>The coupon number associated with the 'inconnection with' document.</xs:documentation>
																		</xs:annotation>
																	</xs:attribute>
																</xs:complexType>
															</xs:element>
														</xs:sequence>
														<xs:attribute name="Status" type="IATA_CodeType" use="optional">
															<xs:annotation>
																<xs:documentation xml:lang="en">The current status of the flight/value coupon.  Refer to IATA PADIS Code List data element 4405.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="CouponItinerarySeqNbr" type="Numeric0to99" use="optional">
															<xs:annotation>
																<xs:documentation>The number representing the order in which this coupon was used or is to be used.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="DeterminingCarrierCode" type="AirlineType">
															<xs:annotation>
																<xs:documentation>The carrier code that was determined to be the applicable carrier code for the free baggage allowance.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="UnitOfMeasureQuantity" type="xs:decimal" use="required">
															<xs:annotation>
																<xs:documentation>This is the numeric value associated with the measurement.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="UnitOfMeasure" type="StringLength1to16" use="optional">
															<xs:annotation>
																<xs:documentation>This is the standard unit of measure name.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="UnitOfMeasureCode" type="IATA_CodeType" use="optional">
															<xs:annotation>
																<xs:documentation>The unit of measure in a code format.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
														<xs:attribute name="FareFamilyName" type="AlphaNumericStringLength10">
															<xs:annotation>
																<xs:documentation>The fare family name chosen by a carrier to designate a bundled offer.</xs:documentation>
															</xs:annotation>
														</xs:attribute>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="AddlBaggageInfo" type="AddlBaggageInfoType" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Carry-on and checked baggage allowances.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="TicketDocumentNbr" type="StringLength1to16" use="required">
										<xs:annotation>
											<xs:documentation xml:lang="en">Document number comprised of the airline code, form code, and serial number.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="Type" type="IATA_CodeType" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">Used to indicate a ticket or EMD.  Refer to IATA PADIS Code List for data element 1001.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="PrimaryDocInd" type="xs:boolean" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">When true, this is the primary ticket/document number. </xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="InConnectionDocNbr" type="StringLength1to16" use="optional"/>
									<xs:attribute name="DateOfIssue" type="xs:date" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">The date a document was issued.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="TimeOfIssue" type="DateOrTimeOrDateTimeType" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">The local time the document was issued in the format HHMM using the 24 hour clock.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="TicketLocation" type="AlphaNumericStringLength1to5" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">City or airport code identifying the ticketing location.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="RoutingDomIntCode" use="optional">
										<xs:annotation>
											<xs:documentation>For tickets, EMD-A and EMD-S with routing, use D if all board and off points within the same country.  If all board and off points are  not within the same country, use  I.  If EMD-S with no routing, do not use.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:NMTOKEN">
												<xs:enumeration value="D">
													<xs:annotation>
														<xs:documentation>Domestic</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="I">
													<xs:annotation>
														<xs:documentation>International</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="InConnectionDocDateOfIssue" type="xs:date">
										<xs:annotation>
											<xs:documentation>The inconnection document date of issue.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="TravelCommencementDate" type="xs:date">
										<xs:annotation>
											<xs:documentation>The date of the first flight of the priced itinerary applied at the document level.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="ExchangeReissue">
										<xs:annotation>
											<xs:documentation>Identifies whether the ticket was the result of an exchange or a reissue of a ticket.</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:NMTOKEN">
												<xs:enumeration value="E">
													<xs:annotation>
														<xs:documentation>The ticket was the result of an exchange.</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration value="R">
													<xs:annotation>
														<xs:documentation>The ticket is the result of a reissue.</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="Commission" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Commission information for this ticket/document.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attributeGroup ref="CommissionGroup">
										<xs:annotation>
											<xs:documentation xml:lang="en">Data for the collection of non-airport/government fees.</xs:documentation>
										</xs:annotation>
									</xs:attributeGroup>
								</xs:complexType>
							</xs:element>
							<xs:element name="FareComponent" type="FareComponentType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Price quote date, account code, input designator, and component count.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="StaffDetail" type="StaffDetailType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>ID number, employing airline, joining date, res entitlement indicator, status, type of travel, ticket type.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="CarrierFeeInfo" type="CarrierFeeInfoType" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Data for the collection of non-airport/government fees.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="ExchResidualFareComponent" minOccurs="0" maxOccurs="99">
								<xs:annotation>
									<xs:documentation>Tfhe reissued residual fare component information.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="FareComponentType">
											<xs:sequence>
												<xs:element name="Taxes" minOccurs="0">
													<xs:annotation>
														<xs:documentation>The tax, fee, or charge amount information. </xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="Tax" maxOccurs="99">
																<xs:complexType>
																	<xs:attributeGroup ref="FeeTaxGroup">
																		<xs:annotation>
