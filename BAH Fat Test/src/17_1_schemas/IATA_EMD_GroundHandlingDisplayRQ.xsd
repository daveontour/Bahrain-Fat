<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.iata.org/IATA/2007/00" targetNamespace="http://www.iata.org/IATA/2007/00" elementFormDefault="qualified" version="1.002" id="IATA2017.1">
	<xs:include schemaLocation="IATA_SimpleTypes.xsd"/>
	<xs:include schemaLocation="IATA_CommonTypes.xsd"/>
	<xs:include schemaLocation="IATA_ET_CommonTypes.xsd"/>
	<xs:element name="IATA_EMD_GroundHandlingDisplayRQ">
		<xs:annotation>
			<xs:documentation>This message requests the display of a previously issued electronic miscellaneous document.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="POS" type="POS_Type">
					<xs:annotation>
						<xs:documentation>The ticket handler system indicator, the ticket handler airline designator, and/or the transaction agent i.d.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ValidatingAirline" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Used to specify the validating carrier to which the display should be routed.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attributeGroup ref="CompanyID_AttributesGroup">
							<xs:annotation>
								<xs:documentation xml:lang="en">The validating carrier to which the display request should be routed is sent in the Code attribute.</xs:documentation>
							</xs:annotation>
						</xs:attributeGroup>
					</xs:complexType>
				</xs:element>
				<xs:element name="TicketDocument" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The EMD number</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="TicketDocumentNbr" type="StringLength1to16" use="required">
							<xs:annotation>
								<xs:documentation xml:lang="en">Document number comprised of the airline code, form code, and serial number.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="DateOfIssue" type="xs:date">
							<xs:annotation>
								<xs:documentation>The date of issue.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="AddlReferenceID" type="UniqueID_Type" minOccurs="0" maxOccurs="2">
					<xs:annotation>
						<xs:documentation>ID contains the actual identifier  - Foid or airline confirmation number.  Refer to IATA PADIS Code List data element 1153 for the code value to send in Type for a Foid and the Code List data element 9958 for the code value for airline confirmation number. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CustLoyalty" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Frequent flyer reference.  ProgramID=airline code, MembershipID=FF number.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="ProgramID" type="StringLength1to16" use="required">
							<xs:annotation>
								<xs:documentation>Used to identify the airline designator of the frequent flyer program.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="MembershipID" type="StringLength1to32" use="required">
							<xs:annotation>
								<xs:documentation>The frequent flyer number.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="DepartureDateTime" type="xs:dateTime" use="required">
							<xs:annotation>
								<xs:documentation>The departure date of the flight.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="Passenger" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Used to request a display by passenger  name or credit card.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Name" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The passenger name.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:complexContent>
										<xs:extension base="PersonNameType">
											<xs:attributeGroup ref="TravelerCountGroup">
												<xs:annotation>
													<xs:documentation>use code for ptc.  </xs:documentation>
												</xs:annotation>
											</xs:attributeGroup>
										</xs:extension>
									</xs:complexContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="SoldAirlineInfo" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Sold date of flight and flight number.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="MarketingAirline" minOccurs="0">
											<xs:annotation>
												<xs:documentation xml:lang="en">The marketing airline.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="CompanyNameType"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="DepartureAirport" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The origin airport/city code.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attributeGroup ref="LocationGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="ArrivalAirport" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The destination airport/city code.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attributeGroup ref="LocationGroup"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="CreditCard" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Credit card vendor and number.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute name="CardCode" type="PaymentCardCodeType" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">The 2 character code of the credit card issuer.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="CardNumber" type="NumericStringLength1to19" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">Credit card number embossed on the card.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="CurrentAirlineInfo" minOccurs="0">
								<xs:annotation>
									<xs:documentation>The current flight information.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="MarketingAirline" minOccurs="0">
											<xs:annotation>
												<xs:documentation xml:lang="en">The marketing airline.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="CompanyNameType"/>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="DepartureAirport" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The origin airport/city code.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attributeGroup ref="LocationGroup"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="ArrivalAirport" minOccurs="0">
											<xs:annotation>
												<xs:documentation>The destination airport/city code.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attributeGroup ref="LocationGroup"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="DepartureDateTime" type="xs:dateTime" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">The current flight date and time of departure.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="FlightNumber" type="FlightNumberType" use="optional">
										<xs:annotation>
											<xs:documentation xml:lang="en">The current flight number.</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="PhoneNumber" type="StringLength1to32" use="optional">
							<xs:annotation>
								<xs:documentation xml:lang="en">The phone number for the passenger.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attributeGroup ref="IATA_PayloadStdAttributes"/>
			<xs:attribute name="Function" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="en">Used to specify whether the request is from an airline or a ground handler.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="Neutral"/>
						<xs:enumeration value="Airline"/>
						<xs:enumeration value="GrHandling"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
</xs:schema>
